omputed:{\n    ...mapState('productInfo',['productDetail']),\n    trustId() {\n      return this.$route.params.id;\n    }\n  },\n  watch:{\n    trustId(trustId) {\n      if (!trustId) {\n        return\n      }\n      this.getTrustDetail(trustId)\n    },\n  },\n  methods:{\n    ...mapMutations('productInfo',['setProductDetail', 'setTrustId']),\n    getTrustDetail(trustId) {\n      var self = this;\n      this.setTrustId(trustId);\n      var svc = this.$http('PortfolioManagement', 'DV.TrustInfo_Get');\n      svc.AddParam({ Name: 'TrustID', Value: trustId, DBType: 'int' });\n      svc.ExecTable().then(res=> {\n        if (!res || res.length<1) {\n          return\n        }\n        self.setProductDetail(res[0]);\n        self.$title(res[0].TrustName);\n\n        const currentTab = this.currentTab;\n        var urls = window.location.href.split('/');\n        var urlC = urls[urls.length - 1]\n        if(currentTab && currentTab.id.indexOf('ProductDetail')>-1 &&  currentTab.params.id === trustId && urlC.toLowerCase() === self.$route.name.toLowerCase()) return;\n        self.titles = res[0].TrustNameShort;\n        console.log(self.$route.name)\n        self.newTab({\n          id: 'ProductDetail'+trustId,\n          title: self.titles,\n          params: {\n            id: trustId\n          },\n          name: self.$route.name,\n        })\n        console.log(this.currentTab)\n      })\n    },\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.productDetail{\n  display: flex;\n  flex: 1;\n  flex-direction: column;\n  padding-left:0;\n  border-radius: 3px;\n  position: relative;\n  height: 100%;\n  .baseInfo{\n    padding: 10px;\n    background-color: rgba(25, 33, 128, 0.78);\n\n    .name{\n      font-size: 16px;\n      color: #fff;\n      margin:0;\n    }\n    .text{\n      margin:0;\n      margin-top: 10px;\n      span{\n        margin-right: 10px;\n        color: rgba(255,255,255,0.6);\n        font-size: 12px;\n        &.datatate{\n          font-size: 14px;\n        }\n      }\n    }\n  }\n  .contentBox{\n    flex:1;\n    display: flex;\n    .leftMenu{\n      flex:0 0 200px;\n      background-color: #3a4489;\n      max-height:100%;\n      li{\n        text-align: center;\n        text-align: left;\n        margin-bottom: 5px;\n        &.productNameTitle{\n          color: #959ed4;\n          text-align: center;\n          padding:13px 0;\n          span{\n            font-weight: bold;\n          }\n          i{\n            margin-right: 5px;\n          }\n        }\n        &.title{\n          padding: 10px 0;\n          color: $--color-grey;\n          font-size: 13px;\n          position: relative;\n          &:before{\n            display: inline-block;\n            content: '';\n            width: 55%;\n            height: 1px;\n            background-color:$--color-grey-border;\n            position: absolute;\n            right: 0;\n            top: 50%;\n          }\n        }\n        a{\n          display: inline-block;\n          width: 100%;\n          padding:13px;\n          padding-left: 63px;\n          text-align: left;\n          color: rgba(#fff, 0.6);\n          position: relative;\n          &:hover{\n            color: #fff;\n          }\n          i{\n            font-size: 30px;\n            position: absolute;\n            top: 3px;\n            right: -11px;\n            display: none;\n            transform: rotate(180deg);\n            color: #f1f1f1;\n          }\n          &.router-link-exact-active.router-link-active{\n            background-color: #4e5aa2;\n            color: #fff;\n            i{\n              display: inline-block;\n            }\n          }\n        }\n      }\n    }\n    .rightRouter{\n      flex:1;\n      background-color: #f1f1f1;\n      padding:15px;\n      overflow: auto;\n      .detailPage{\n        padding:0;\n      }\n    }\n  }\n}\n\n</style>\n"]}]}