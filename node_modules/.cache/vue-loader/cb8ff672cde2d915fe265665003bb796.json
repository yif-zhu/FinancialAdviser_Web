\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n\n.bondDetailBox{\n  li{\n    width: 33%;\n    display: inline-block;\n  }\n  p{\n    line-height: 30px;\n    span:nth-child(1){\n      color: $--color-grey;\n      display: inline-block;\n      width: 50%;\n    }\n  }\n}\n  .tabs{\n    .el-tabs__nav-wrap::after{\n      background-color: #fff;\n    }\n    .el-tabs__active-bar{\n      height: 3px;\n      background-color: transparent;\n      &:before{\n        position: absolute;\n        content: \"\";\n        left: 50%;\n        right: 50%;\n        width: 20px;\n        margin-left: -10px;\n        margin-right: -10px;\n        height: 3px;\n        background-color: $--color-primary;\n      }\n    }\n\n    .el-tabs__item{\n      color: $--color-grey;\n      font-size: 13px;\n      &:hover{\n        color: $--color-black;\n      }\n      &.is-active{\n        font-size: 16px;\n        color: $--color-black;\n      }\n    }\n\n    .el-tabs__content{\n      overflow: visible;\n    }\n    .analysistabs{\n      text-align:center;\n      }\n  }\n",{"version":3,"sources":["SecuritiesInformation.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAggBA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA","file":"SecuritiesInformation.vue","sourceRoot":"src/views/product","sourcesContent":["\n<template>\n  <div class=\"detailPage\">\n    <el-tabs v-model=\"activeName\" class=\"tabs\" @tab-click=\"handleClick\">\n      <el-tab-pane label=\"全部\" name=\"first\">\n        <ItemBox title=\"概况\">\n          <template slot=\"right\">\n            <el-popover\n              placement=\"left\"\n              width=\"400\"\n              trigger=\"click\">\n              <BaseInfo :line='1'>\n                <li><span style=\"flex: 1\">产品总资产/总发行规模:</span><span>{{''+LayeredDetailInfos20+'%'}}</span></li>\n                <li><span style=\"flex: 1\">产品总资产/优先级发行规模:</span><span>{{''+LayeredDetailInfos21+'%'}}</span></li>\n                <li><span style=\"flex: 1\">产品总资产/（优先级+次级发行规模）:</span><span>{{''+LayeredDetailInfos22+'%'}}</span></li>\n              </BaseInfo>\n              <el-button slot=\"reference\" @click=\"viewRatioInfo\" size=\"mini\">查看比例信息</el-button>\n            </el-popover>\n          </template>\n\n\n\n          <el-table :data=\"bondsInfo\" :show-header=\"false\" style=\"width: 100%\">\n            <el-table-column width=\"170\">\n              <template slot-scope=\"scope\">\n                <strong>{{scope.row.itemName}}</strong>\n              </template>\n            </el-table-column>\n            <el-table-column v-for=\"(item, index) in tableCols\"\n              min-width=\"120\"\n             :prop=\"'column_'+index\">\n            </el-table-column>\n          </el-table>\n        </ItemBox>\n      </el-tab-pane>\n      <el-tab-pane v-for=\"item in SecuritiesBase\" :label=\"item.ShortName\" :name=\"String(item.TrustBondId)\">\n        <ItemBox title=\"基本信息\" class=\"bondDetailBox\">\n          <BaseInfo v-if=\"bondDetail.length>0\" :line='3'>\n            <li v-for=\"items in bondDetail\">\n              <span>{{items.ItemAliasValue}}</span>\n              <span>{{items.ItemValue}}</span>\n            </li>\n          </BaseInfo>\n        </ItemBox>\n        <ItemBox title=\"图表信息\" class=\"bondDetailBox\">\n          <div style=\"display:flex;\">\n            <div style=\"flex:1;\">\n              <el-table :data=\"bondChartDetail\" height=\"315\">\n                <el-table-column prop=\"TransDate\" label=\"日期\">\n                  <template slot-scope=\"scope\">\n                    <span>{{scope.row.TransDate.S2Date()}}</span>\n                  </template>\n                </el-table-column>\n                <el-table-column prop=\"TransVolume\" label=\"成交份额\"></el-table-column>\n                <el-table-column prop=\"TransAmount\" label=\"成交金额(元)\" :formatter=\"stateFormat\">\n                </el-table-column>\n                <el-table-column prop=\"CalculatedPrice\" label=\"全价(元)\" :formatter=\"stateFormat\">\n\n                </el-table-column>\n              </el-table>\n            </div>\n            <div :id=\"'bondChart'+item.TrustBondId\" style=\"flex:0 0 400px;\"></div>\n          </div>\n        </ItemBox>\n        <ItemBox v-if=\"principalSchedule.length\" title=\"还本计划\" class=\"bondDetailBox\">\n          <el-table :data=\"principalSchedule\" height=\"300\">\n\t\t\t\t\t\t<el-table-column prop=\"ScheduleDate\" label=\"日期\">\n\t\t\t\t\t\t</el-table-column>\n\t\t\t\t\t\t<el-table-column prop=\"ScheduleAmount\" label=\"金额\">\n\t\t\t\t\t\t</el-table-column>\n\t\t\t\t\t</el-table>\n        </ItemBox>\n      </el-tab-pane>\n    </el-tabs>\n     <ItemBox title=\"期限敏感性分析-违约率假设\">\n          <el-table :data=\"tableData\" class=\"analysistabs\" style=\"width: 100%\">\n             <el-table-column\n            prop=\"0\"\n            label=\"违约率假设\"\n            width=\"150\">\n          </el-table-column>\n            <el-table-column label=\"WAL(年)\">\n            <el-table-column v-if=\"column1IsHide\"\n              :label=\"column1ShortName\"\n              prop=\"1\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column2IsHide\"\n              :label=\"column2ShortName\"\n              prop=\"2\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column3IsHide\"\n             :label=\"column3ShortName\"\n              prop=\"3\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column4IsHide\"\n             :label=\"column4ShortName\"\n              prop=\"4\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column5IsHide\"\n              :label=\"column5ShortName\"\n              prop=\"5\"\n              >\n            </el-table-column>\n          </el-table-column>\n          </el-table>\n        </ItemBox>\n     <ItemBox title=\"期限敏感性分析-早偿率假设\">\n          <el-table :data=\"analysistableData\" class=\"analysistabs\" style=\"width: 100%\">\n             <el-table-column\n            prop=\"0\"\n            label=\"早偿率假设\"\n            width=\"150\">\n          </el-table-column>\n            <el-table-column label=\"WAL(年)\">\n            <el-table-column v-if=\"column1IsHide\"\n              :label=\"column1ShortName\"\n              prop=\"1\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column2IsHide\"\n              :label=\"column2ShortName\"\n              prop=\"2\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column3IsHide\"\n             :label=\"column3ShortName\"\n              prop=\"3\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column4IsHide\"\n             :label=\"column4ShortName\"\n              prop=\"4\"\n              >\n            </el-table-column>\n            <el-table-column v-if=\"column5IsHide\"\n              :label=\"column5ShortName\"\n              prop=\"5\"\n              >\n            </el-table-column>\n          </el-table-column>\n          </el-table>\n        </ItemBox>\n  </div>\n</template>\n\n<script>\n  // import MarketInfoTable from '@/components/overview/marketInfo/MarketInfoTable';\n  import Highcharts from 'highcharts';\n  import BaseInfo from '@/components/common/BaseInfo.vue';\n  import ItemBox from '@/components/layout/ItemBox.vue';\n  import utilsIndex from '@/utils/index.js'\n  export default {\n    name: \"BaseInfoTable\",\n    // components: { MarketInfoTable},\n    data() {\n      return {\n        SecuritiesBase: [],\n        activeName: 'first',\n        bondDetail:[],\n        bondChartDetail:[],\n\t\t\t\tprincipalSchedule: [],\n        LayeredDetailInfos20: '',\n        LayeredDetailInfos21: '',\n        LayeredDetailInfos22: '',\n        bondsInfo: [],\n        tableCols: 0,\n        tableData:[],\n        analysistableData:[],\n        column1IsHide:false,\n        column1ShortName:'',\n        column2IsHide:false,\n        column2ShortName:'',\n        column3IsHide:false,\n        column3ShortName:'',\n        column4IsHide:false,\n        column4ShortName:'',\n        column5IsHide:false,\n        column5ShortName:'',\n\n      }\n    },\n    components:{\n      BaseInfo,\n      ItemBox\n    },\n    created() {\n      this.initSecuritiesBase();\n      var self = this;\n     var svc3 = this.$http('PortfolioManagement', 'DV.TrustBond_Get');\n        svc3.AddParam({\n          Name: 'TrustID',\n          Value: this.$store.state.productInfo.trustId,\n          DBType: 'int'\n        });\n        svc3.ExecTable().then(res => {\n          console.log('column1IsHide')\n          console.log(res.length)\n        if (res.length>0) {\n          for (var i = 0; i < res.length; i++) {\n            if (i==0) {self.column1IsHide=true,self.column1ShortName=res[0].ShortName}\n            if (i==1) {self.column2IsHide=true,self.column2ShortName=res[1].ShortName}\n            if (i==2) {self.column3IsHide=true,self.column3ShortName=res[2].ShortName}\n            if (i==3) {self.column4IsHide=true,self.column4ShortName=res[3].ShortName}\n            if (i==4) {self.column5IsHide=true,self.column6ShortName=res[4].ShortName}\n             }\n           }\n        });\n\n       var svc = this.$http(\"PortfolioManagement\", \"DV.usp_getTermSensitivityAnalysis\");\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: this.$store.state.productInfo.trustId,\n          DBType: 'int'\n        });\n        svc.AddParam({\n          Name: 'AnalysisType',\n          Value: 0,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          if (res.length>0) {\n             this.tableData = res;\n\n          }\n        });\n\n       var svc2 = this.$http(\"PortfolioManagement\", \"DV.usp_getTermSensitivityAnalysis\");\n        svc2.AddParam({\n          Name: 'TrustID',\n          Value: this.$store.state.productInfo.trustId,\n          DBType: 'int'\n        });\n        svc2.AddParam({\n          Name: 'AnalysisType',\n          Value: 1,\n          DBType: 'int'\n        });\n        svc2.ExecTable().then(res => {\n          if (res.length>0) {\n             this.analysistableData = res;\n\n          }\n        });\n    },\n    computed: {\n      TrustID() {\n        return this.$store.state.productInfo.trustId;\n      }\n    },\n    methods: {\n      stateFormat(row, column, cellValue) {\n        return utilsIndex.numUnit(cellValue)\n      },\n      stateFormatW0(row, column, cellValue) {\n        if(cellValue == null){\n          return 0\n        }else{\n          cellValue = (Number(cellValue)/10000).toFixed(2);\n          cellValue += \"\";\n          if (!cellValue.includes(\".\")) cellValue += \".\";\n          return cellValue\n            .replace(/(\\d)(?=(\\d{3})+\\.)/g, function($0, $1) {\n              return $1 + \",\";\n            })\n            .replace(/\\.$/, \"\");\n        }\n\n      },\n      stateFormatW(row, column, cellValue) {\n        if(cellValue == null){\n          return ''\n        }else{\n          cellValue = (Number(cellValue)/10000).toFixed(2);\n          cellValue += \"\";\n          if (!cellValue.includes(\".\")) cellValue += \".\";\n          return cellValue\n            .replace(/(\\d)(?=(\\d{3})+\\.)/g, function($0, $1) {\n              return $1 + \",\";\n            })\n            .replace(/\\.$/, \"\");\n        }\n      },\n      handleClick(tab, event) {\n        if (this.activeName == 'first') {\n          return\n        }\n        this.getBond(tab.name)\n        this.getBondChart(tab.name)\n      },\n      getBondChart(id) {\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.usp_GetBondTranscationStatistics');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID,\n          DBType: 'int'\n        });\n        svc.AddParam({\n          Name: 'TrustBondID',\n          Value: id,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          this.bondChartDetail = res;\n          let x = [],y = [];\n          if (res && res.length) {\n            for (var i = 0; i < res.length; i++) {\n              x.push(res[i].TransDate.S2Date())\n              y.push(res[i].CalculatedPrice)\n            }\n          }\n          let yAr = [{\n            name:'报价走势',\n            data:y\n          }]\n          this.highChartFun(x,yAr,id)\n        })\n      },\n      viewRatioInfo(){\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.usp_getLayeredDetailInfos');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          self.LayeredDetailInfos20 = self.isDot(res[0][20]);\n          self.LayeredDetailInfos21 = self.isDot(res[0][21]);\n          self.LayeredDetailInfos22 = self.isDot(res[0][22]);\n        })\n      },\n      initSecuritiesBase() {\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.TrustBond_Get');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          this.SecuritiesBase = JSON.parse(JSON.stringify(res));\n          // 数据横转竖\n          let rows = res.length, data = [];\n          let mapCN = {\n            SecurityExchangeCode: '证券代码',\n            ShortName: '证券简称',\n            ClassTypeName: '债券类别',\n            ClassName: '当前评级',\n            yspj: \"原始评级\",\n            OfferAmount:\"发行规模(万元)\" ,\n            CouponPaymentReference:\"利率形式\",\n            CouponBasis:\"发行利率(%)\",\n            CurrentRate: \"当前利率(%)\",\n            BaseRate: \"基准利率(%)\",\n            Spread: \"利差(%)\",\n            LegalMaturityDate: \"预计到期日\",\n            EndDate: \"实际到期日\",\n            PaymentConvention: \"还本方式\",\n            InterestPaymentTypeName: \"付息方式\",\n            PayAmount: \"已付本金金额(万元)\",\n            PaidPercentage: \"已付本金占比(%)\",\n            //RemainingBalance: \"每份剩余(元)\",\n            ClosingAmount: \"剩余本金余额（万元）\",\n            mz: \"每份面值\",\n            syqx: \"剩余期限（月）\"\n          }\n          this.tableCols = rows;\n          if(res.length) {\n            res = res.map(v => {\n              v.Spread = v.CurrentRate !== null && v.BaseRate !== null?(v.CurrentRate - v.BaseRate).toFixed(2):'--'\n              return v;\n            })\n            let fields = Object.keys(res[0]);\n            let cols = fields.length;\n            for(let c = 0; c < cols; c++) {\n              let obj = {};\n              let field = fields[c];\n              if(!mapCN[field]) {\n                continue;\n              }\n              obj.itemName = mapCN[field];\n              for (let t = 0; t < rows; t++) {\n                let val = res[t][field];\n                // 处理空值或无法直接显示的值\n                if(field === 'EndDate') {\n                  val = val ? val.S2Date() : '--';\n                }else if(field === 'RemainingBalance') {\n                  val = this.stateFormat(0, 0, val);\n                }else if(field === 'OfferAmount' || field === 'ClosingAmount') {\n                  val = this.stateFormatW(0, 0, val);\n                }else if(field === 'PayAmount') {\n                  val = this.stateFormatW0(0, 0, val);\n                }else if(field === 'CouponBasis' ||field === 'CurrentRate' ||field === 'BaseRate') {\n                  val = !utilsIndex.isNull(val) ?Number(val).toFixed(2):'--'\n                }\n                obj['column_'+t] = val;\n              }\n              data.push(obj);\n            }\n          }\n          // 最后再按规则排下序\n          let sorts = Object.values(mapCN);\n          data.sort((v1, v2) => {\n            return sorts.indexOf(v1.itemName) - sorts.indexOf(v2.itemName)\n          })\n          this.bondsInfo = data;\n        })\n      },\n      getBond(id) {\n        console.log(id)\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.TrustBondInfo_Get');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID,\n          DBType: 'int'\n        });\n        svc.AddParam({\n          Name: 'TrustBondId',\n          Value: id,\n          DBType: 'int'\n        });\n        svc.ExecTables().then(res => {\n\t\t\t\t\t//let data = res[0], len = data.length;\n          this.bondDetail = [];\n          this.principalSchedule = [];\n          for (var a = 0; a < res.length; a++) {\n            if (res[a][0].ScheduleDate) {\n              this.principalSchedule = res[a]\n            }else{\n              this.dataFun(res[a])\n            }\n          }\n        })\n      },\n      dataFun(data) {\n        for (var i = 0; i < data.length; i++) {\n          if(data[i].ItemCode == \"OfferAmount\" || data[i].ItemCode == \"Denomination\"){\n            data[i].ItemValue = data[i].ItemValue?Number(data[i].ItemValue).toLocaleString():data[i].ItemValue\n          }\n          if(data[i].ItemCode == \"CouponBasis\" || data[i].ItemCode == \"CurrentRate\"){\n            data[i].ItemValue = data[i].ItemValue?Number(data[i].ItemValue).toFixed(2):data[i].ItemValue\n          }\n          if(data[i].ItemCode === 'IsChinabondDelimit') {\n            if(data[i].ItemValue) data[i].ItemValue = Boolean(data[i].ItemValue) ? '是' : '否';\n          }\n          if (data[i].ItemValue == \"\" || !data[i].ItemValue) {\n            if (data[i].ItemValue !== 0) {\n              data[i].ItemValue = '-'\n            }\n          }\n        }\n        this.bondDetail = this.bondDetail.concat(data)\n      },\n      highChartFun(categoriesX,data,id) {\n        console.log(categoriesX,data)\n        let chart = Highcharts.chart('bondChart'+id, {\n          chart: {\n            type: 'spline'\n          },\n          title: {\n            text: ''\n          },\n          subtitle: {\n            text: ''\n          },\n          yAxis: {\n            title: {\n              text: '全价（元）'\n            }\n          },\n          xAxis: {\n            categories:categoriesX\n          },\n          exporting: { enabled: false },//隐藏导出图片\n          credits: { enabled: false },//隐藏highcharts的站点标志\n          plotOptions: {\n            // series: {\n            //   label: {\n            //     connectorAllowed: false\n            //   },\n            //   pointStart: 2010\n            // }\n          },\n          series: data,\n\n        });\n        Highcharts.setOptions({\n          lang: {\n            thousandsSep: ','\n          }\n        });\n      },\n      isDot(num) {\n        let result = (num.toString()).indexOf(\".\");\n        if(result != -1) {\n          return num.toFixed(2)\n        } else {\n          return num\n        }\n      }\n    }\n  }\n\n</script>\n\n<style lang=\"scss\">\n.bondDetailBox{\n  li{\n    width: 33%;\n    display: inline-block;\n  }\n  p{\n    line-height: 30px;\n    span:nth-child(1){\n      color: $--color-grey;\n      display: inline-block;\n      width: 50%;\n    }\n  }\n}\n  .tabs{\n    .el-tabs__nav-wrap::after{\n      background-color: #fff;\n    }\n    .el-tabs__active-bar{\n      height: 3px;\n      background-color: transparent;\n      &:before{\n        position: absolute;\n        content: \"\";\n        left: 50%;\n        right: 50%;\n        width: 20px;\n        margin-left: -10px;\n        margin-right: -10px;\n        height: 3px;\n        background-color: $--color-primary;\n      }\n    }\n\n    .el-tabs__item{\n      color: $--color-grey;\n      font-size: 13px;\n      &:hover{\n        color: $--color-black;\n      }\n      &.is-active{\n        font-size: 16px;\n        color: $--color-black;\n      }\n    }\n\n    .el-tabs__content{\n      overflow: visible;\n    }\n    .analysistabs{\n      text-align:center;\n      }\n  }\n</style>\n"]}]}