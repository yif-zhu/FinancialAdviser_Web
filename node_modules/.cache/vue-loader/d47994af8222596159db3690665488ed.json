                  <el-form ref=\"form\" :model=\"form\" label-width=\"70px\">\n                    <el-form-item label=\"数值\">\n                      <el-select v-model=\"form.primary\" placeholder=\"请选择\">\n                        <el-option value=\"LoanCount\" label=\"户数\"></el-option>\n                        <el-option value=\"AssetDebtCount\" label=\"标的资产债权笔数\"></el-option>\n                        <el-option value=\"AssetDebtAmount\" label=\"标的资产债权本息余额\"></el-option>\n                        <el-option value=\"RedemptionDebtCount\" label=\"赎回（标的资产债权笔数）\"></el-option>\n                        <el-option value=\"RedemptionDebtAmount\" label=\"赎回（标的资产债权金额）\"></el-option>\n                        <el-option value=\"RedemptionDebtAmount\" label=\"赎回（标的资产债权占比）\"></el-option>\n                      </el-select>\n                    </el-form-item>\n                    <el-form-item>\n                      <el-button type=\"primary\" @click=\"onSubmitNPL(index)\">确定</el-button>\n                      <el-button @click=\"closeEdit(index)\">取消</el-button>\n                    </el-form-item>\n                  </el-form>\n                </el-tab-pane>\n              </el-tabs>\n            </div>\n            </div>\n          </div>\n        </div>\n\n\n      </div>\n    </ItemBox> -->\n    <div v-if=\"loadingTitleS\" class=\"noData\" v-loading=\"loadingTitleS\" style=\"height:300px;\" element-loading-text=\"拼命加载中\"></div>\n  </div>\n  </div>\n</template>\n\n<script>\n  import ItemBox from '@/components/layout/ItemBox.vue';\n  import Highcharts from 'highcharts';\n  import Exporting from 'highcharts/modules/exporting'\n  import OfflineExporting from 'highcharts/modules/offline-exporting'\n  import {addClass,removeClass} from '@/utils/dom.js'\n  import BaseInfo from '@/components/common/BaseInfo.vue'\n  import {mapState, mapMutations} from 'vuex';\n  Exporting(Highcharts);\n  OfflineExporting(Highcharts);\n  export default {\n    data() {\n      return {\n        assetPoolStatistics: [],\n        distributionData: [],\n        activeName:'column',\n        domIndex:0,\n        cartName:'column',\n        form:{\n          primary:'',\n          primaryLine:'column',\n          secondary:'',\n          secondaryLine:'column'\n\n        },\n        tableMenuS:false,\n        loadingTitleS:true,\n        CurrentPeriodSel:1,\n        // 不良资产\n        distributionDataNPL:[],\n        distributionDataNPLs:[]\n      }\n    },\n    watch: {\n      AssetType(v) {\n        // if (v == 'NPL') {\n        // this.initDistributionsNPL()\n        // }\n        this.initDistributions()\n      },\n      distributionData(v) {\n        this.distributionFun(v)\n      },\n      distributionDataNPLs: {\n        handler(nv,ov) {\n          console.log(nv)\n          this.distributionFunNPL(nv,'标的资产债权本息余额')\n        },\n\n      },\n\n    },\n    components:{\n      BaseInfo,\n      ItemBox\n    },\n\n    created(){\n      this.CurrentPeriodSel = this.CurrentPeriodAr.length>0?this.CurrentPeriodAr[0]:0;\n      this.initPoolStatisticsData();\n      this.initDistributions()\n      // if (this.AssetType == 'NPL') {\n      //   this.initDistributionsNPL()\n      // }\n    },\n    computed: {\n      ...mapState('productInfo',['productDetail']),\n      AssetType() {\n        return this.productDetail.AssetType\n      },\n      CurrentPeriodAr() {\n        var CurrentPeriodAr = []\n        var CurrentPeriod = Number(this.$route.query.CurrentPeriod);\n        if (CurrentPeriod) {\n          for (var i = 0; i < CurrentPeriod+1; i++) {\n            CurrentPeriodAr.push(i)\n          }\n        }\n        return CurrentPeriodAr;\n      },\n      TrustID() {\n        return this.$store.state.productInfo.trustId;\n      },\n\n    },\n    methods: {\n      numUnit(num){\n        let money = num && num != 'ND'?Number(num).toFixed(2).toString().replace(/(\\d{1,3})(?=(\\d{3})+(?:$|\\.))/g, \"$&,\"):num\n        return money;\n      },\n      // 不良资产\n      initDistributionsNPL() {\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.usp_GetPoolArrearsPerformanceByTrustId');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID, //4513,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          this.loadingTitleS = false\n          var data = res\n          for (var i = 0; i < data.length; i++) {\n            data[i].y  = data[i].AssetDebtAmount\n            data[i].name = data[i].Bucket\n          }\n          this.distributionDataNPL = data\n          var dataS = data.filter((item) => {\n            return item.PaymentPeriod == self.CurrentPeriodSel\n          })\n          if (dataS.length>0) {\n            this.distributionDataNPLs.push(dataS)\n          }\n\n          console.log(this.distributionDataNPLs)\n        })\n      },\n      PeriodChange(value) {\n        this.initPoolStatisticsData(value)\n        // if (this.AssetType == 'NPL') {\n        //   this.distributionDataNPLs = []\n        //   var dataS = this.distributionDataNPL.filter((item) => {\n        //     return item.PaymentPeriod == this.CurrentPeriodSel\n        //   })\n        //   if (dataS.length>0) {\n        //     this.distributionDataNPLs.push(dataS)\n        //   }\n        // }\n        this.initDistributions(value);\n      },\n      distributionFun(v) {\n        this.$nextTick(() => {\n          if (v.length < 1) {\n            return\n          }\n          for (var i = 0; i < v.length; i++) {\n            this.paintCart(i, v[i].slice(0, -1), v[i][v[i].length - 1].Count + '笔')\n          }\n        })\n      },\n      distributionFunNPL(v,name) {\n        this.$nextTick(() => {\n          for (var i = 0; i < v.length; i++) {\n            if (v[i].length>0) {\n              this.paintCart(i, v[i], name)\n            }\n          }\n        })\n      },\n      scrollClick(index) {\n        document.getElementsByClassName('contentBox')[0].scrollTop = document.getElementsByClassName('itemBox'+index)[0].offsetTop - 70\n      },\n      initDistributions(CurrentPeriodSel) {\n        var PaymentPeriodID;\n        if (CurrentPeriodSel) {\n          PaymentPeriodID = CurrentPeriodSel\n        }else{\n          PaymentPeriodID = this.CurrentPeriodSel\n        }\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.PoolDistributions_Get');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID, //4513,\n          DBType: 'int'\n        });\n        svc.AddParam({\n          Name: 'PaymentPeriodID',\n          Value: PaymentPeriodID, //4513,\n          DBType: 'int'\n        });\n        svc.ExecTable().then(res => {\n          var arr = res\n          console.log(res)\n          var map = {},\n            dest = [];\n          for (var i = 0; i < arr.length; i++) {\n            var ai = arr[i];\n            if (!map[ai.DistributionTypeCode]) {\n              dest.push([{\n                Amount: ai.Amount,\n                AmountPercentage: ai.AmountPercentage,\n                Bucket: ai.Bucket,\n                BucketSequenceNo: ai.BucketSequenceNo,\n                Count: ai.Count,\n                CountPercentage: ai.CountPercentage,\n                DistributionTypeCode: ai.DistributionTypeCode,\n                ItemTitle: ai.ItemTitle,\n                PoolID: ai.PoolID,\n                ReportingDateID: ai.ReportingDateID\n              }]);\n              map[ai.DistributionTypeCode] = ai;\n            } else {\n              for (var j = 0; j < dest.length; j++) {\n                var dj = dest[j];\n                if (dj[0].DistributionTypeCode == ai.DistributionTypeCode) {\n                  dj.push(ai);\n                  break;\n                }\n              }\n            }\n          }\n          for (var i = 0; i < dest.length; i++) {\n            var amount = 0,\n              count = 0,\n              CountPercentagecount=0,\n              AmountPercentagecount=0;\n            for (var a = 0; a < dest[i].length; a++) {\n              dest[i][a].name = dest[i][a].Bucket;\n              dest[i][a].y = dest[i][a].AmountPercentage;\n              dest[i][a].Amounts = dest[i][a].Amount;\n              dest[i][a].Counts = dest[i][a].Count;\n              amount += dest[i][a].Amount;\n              count += dest[i][a].Count;\n              CountPercentagecount+=dest[i][a].CountPercentage;\n              AmountPercentagecount+=dest[i][a].AmountPercentage;\n\n            }\n            dest[i].push({\n              Amount: amount,\n              Count: count,\n              CountPercentage: CountPercentagecount>100?100:CountPercentagecount,\n              AmountPercentage: AmountPercentagecount>100?100: AmountPercentagecount,\n              Bucket: '合计',\n              Amounts: amount,\n              Counts: count,\n            })\n          }\n          console.log(dest)\n          this.distributionData = dest\n\n          this.loadingTitleS = false\n\n        })\n      },\n      initPoolStatisticsData(CurrentPeriodSel) {\n        var PaymentPeriodID;\n        if (CurrentPeriodSel) {\n          PaymentPeriodID = CurrentPeriodSel\n        }else{\n          PaymentPeriodID = this.CurrentPeriodSel\n        }\n        var self = this;\n        var svc = this.$http('PortfolioManagement', 'DV.PoolStatistics_Get');\n        svc.AddParam({\n          Name: 'TrustID',\n          Value: self.TrustID, //4513,\n          DBType: 'int'\n        });\n        svc.AddParam({\n          Name: 'PaymentPeriodID',\n          Value: PaymentPeriodID, //4513,\n          DBType: PaymentPeriodID == ''?'string':'int'\n        });\n        svc.ExecTable().then(res => {\n          var reg=/\\d{1,3}(?=(\\d{3})+$)/g;\n          if (res && res.length) {\n            var data = res.map(v => {\n              let key = v.ItemCode;\n              if (key == 'CurrentPrincipalBalance' ||key == 'AverageCustomerBalance' ||key == 'TotalAprovalAmount' || key == 'AverageLoanBalance' ||key == 'MaxLoanBalance' ||key == 'MinLoanBalance' ||key == 'TotalApprovalAmount' ||key == 'TotalBalance' || key == 'MaxApprovalAmount' || key == 'AverageApprovalAmount' || key == 'MinApprovalAmount') {\n                v.ItemValue = v.ItemValue !== 'ND' ? self.numUnit(v.ItemValue) : v.ItemValue\n              }\n              v.ItemValue = v.ItemValue !== null ? v.ItemValue : 'ND'\n              return v;\n            })\n            this.assetPoolStatistics = data\n          }else{\n            this.assetPoolStatistics = []\n          }\n          console.log(this.assetPoolStatistics)\n        })\n      },\n      paintCart(el, data, pieTitle) {\n        var pointf = '',titleT=''\n        if (this.AssetType == 'NPL') {\n          pointf = `户数: <b>{point.LoanCount}</b><br>\n                      标的资产债权笔数：<b>{point.AssetDebtCount}</b><br>\n                      标的资产债权本息余额: <b>{point.AssetDebtAmount}</b><br>\n                      赎回（标的资产债权笔数）: <b>{point.RedemptionDebtCount}</b><br>\n                      赎回（标的资产债权金额）: <b>{point.RedemptionDebtAmount}</b><br>\n                      赎回（标的资产债权占比）：<b>{point.DebtPercentage:.2f}%</b>`;\n          titleT = pieTitle;\n        }else{\n          pointf = `本金金额: <b>{point.Amounts:.,0f}</b><br>\n                      本金金额占比：<b>{point.AmountPercentage:.2f}%</b><br>\n                      笔数: <b>{point.Counts}</b><br>\n                      笔数占比：<b>{point.CountPercentage:.2f}%</b>`;\n          titleT = '合计：'+pieTitle;\n        }\n        var chart = Highcharts.chart('chartDiv' + el, {\n          chart: {\n            spacing: [40, 0, 40, 0],\n          },\n          title: {\n            floating: true,\n            text: titleT,\n            style: {\n              fontSize: '13px'\n            }\n          },\n          exporting: {\n            enabled:false\n          },\n          credits: {\n            enabled:false\n          },\n          tooltip: {\n            pointFormat: pointf\n          },\n          plotOptions: {\n            pie: {\n              allowPointSelect: true,\n              cursor: 'pointer',\n              size:180,\n              showInLegend: true,\n              dataLabels: {\n                enabled: false,\n              },\n              point: {\n                events: {\n                  mouseOver: function (e) {\n                    chart.setTitle({\n                      // text: e.target.name+ '\\t'+ e.target.y + ' %'\n                    });\n                  }\n                }\n              },\n            }\n          },\n          series: [{\n            type: 'pie',\n            innerSize: '80%',\n            name: '市场份额',\n            data: data\n          }]\n        }, function (c) { // 图表初始化完毕后的会掉函数\n          // 环形图圆心\n          var centerY = c.series[0].center[1],\n            titleHeight = parseInt(c.title.styles.fontSize);\n          // 动态设置标题位置\n          c.setTitle({\n            y: centerY + titleHeight / 2\n          });\n        });\n        Highcharts.setOptions({\n          lang: {\n            thousandsSep: ','\n          }\n        });\n        document.getElementById('downSpan'+el).onclick = function() {\n          chart.exportChart({\n            type: 'image/png',\n            filename: 'highcharts'+el\n          });\n        }\n      },\n      columnCart(index,xaxis,data) {\n        console.log(index,xaxis,data)\n        var chart = Highcharts.chart('chartDiv' + index,{\n          chart: {\n\n          },\n          title: {\n            text: ''\n          },\n          exporting: {\n            enabled:false\n          },\n          credits: {\n            enabled:false\n          },\n          subtitle: {\n            text: ''\n          },\n          xAxis: {\n            categories: xaxis,\n            crosshair: true\n          },\n          yAxis: [{\n            labels: {\n              format: '{value}',\n              style: {\n                color: '#89A54E'\n              }\n            },\n            title: {\n              text: '',\n              style: {\n                color: '#89A54E'\n              }\n            }\n        }, {\n            title: {\n                text: '',\n                style: {\n                    color: '#4572A7'\n                }\n            },\n            labels: {\n                format: '{value}',\n                style: {\n                    color: '#4572A7'\n                }\n            },\n\n            opposite: true\n\n        }],\n\n          tooltip: {\n            // head + 每个 point + footer 拼接成完整的 table\n            // headerFormat: '',\n\n            footerFormat: '',\n            shared: true,\n            useHTML: true\n          },\n          plotOptions: {\n            column: {\n              borderWidth: 0\n            }\n          },\n          series: data\n        });\n        Highcharts.setOptions({\n          lang: {\n            thousandsSep: ','\n          }\n        });\n        document.getElementById('downSpan'+index).onclick = function() {\n          chart.exportChart({\n            type: 'image/png',\n            filename: 'highcharts'+index\n          });\n        }\n      },\n      editFun(index) {\n        // var boxLen = this.AssetType == 'NPL'? this.distributionDataNPLs:this.distributionData\n        var boxLen = this.distributionData\n        for (var i = 0; i < boxLen; i++) {\n          document.getElementsByClassName('el-tabs')[i].style.display = 'none'\n          document.getElementsByClassName('editBox'+i)[0].style.height = 'auto'\n        }\n        document.getElementById('tabCart'+index).style.display = 'block';\n        document.getElementsByClassName('editBox'+index)[0].style.height = '100%'\n        this.domIndex = index\n      },\n      closeEdit(index) {\n        document.getElementById('tabCart'+index).style.display = 'none'\n        document.getElementsByClassName('editBox'+index)[0].style.height = 'auto'\n      },\n      handleClick(tab,event) {\n        this.cartName = tab.name\n      },\n      columnData(index,cartType,form) {\n        var data = this.distributionData[index].slice(0,-1)\n        console.log(data)\n        var xz = data.map((item) => {\n          return item.Bucket\n        })\n        var yz1 = data.map((item) => {\n          return item[form.primary]\n        })\n        var yz2 = data.map((item) => {\n          return item[form.secondary]\n        })\n        var n1 = form.primary == 'Amount'?'本金金额':form.primary == 'AmountPercentage'?'本金金额占比':form.primary == 'Count'?'笔数':'笔数占比';\n        var n2 = form.secondary == 'Amount'?'本金金额':form.secondary == 'AmountPercentage'?'本金金额占比':form.secondary == 'Count'?'笔数':'笔数占比'\n        var ydata = []\n        ydata.push({\n          type:form.primaryLine,\n          name:n1,\n          data:yz1\n        })\n        ydata.push({\n          type:form.secondaryLine,\n          name:n2,\n          data:yz2,\n          yAxis: 1\n        })\n        this.columnCart(this.domIndex,xz,ydata)\n      },\n      columnDataNPL(index,cartType,form) {\n        var data = this.distributionDataNPLs[index]\n        console.log(data)\n        var xz = data.map((item) => {\n          return item.Bucket\n        })\n        var yz1 = data.map((item) => {\n          return item[form.primary]\n        })\n        var yz2 = data.map((item) => {\n          return item[form.secondary]\n        })\n        var n1 = '';\n        if (this.form.primary.indexOf('LoanCount')>-1) {\n          n1 = '户数'\n        }else if (this.form.primary.indexOf('AssetDebtCount')>-1){\n          n1 = '标的资产债权笔数'\n        }else if (this.form.primary.indexOf('AssetDebtAmount')>-1){\n          n1 = '标的资产债权本息余额'\n        }else if (this.form.primary.indexOf('RedemptionDebtCount')>-1){\n          n1 = '赎回（标的资产债权笔数）'\n        }else if (this.form.primary.indexOf('RedemptionDebtAmount')>-1){\n          n1 = '赎回（标的资产债权金额）'\n        }else if (this.form.primary.indexOf('DebtPercentage')>-1){\n          n1 = '赎回（标的资产债权占比）'\n        }\n        var n2 = '';\n        if (this.form.secondary.indexOf('LoanCount')>-1) {\n          n2 = '户数'\n        }else if (this.form.secondary.indexOf('AssetDebtCount')>-1){\n          n2 = '标的资产债权笔数'\n        }else if (this.form.secondary.indexOf('AssetDebtAmount')>-1){\n          n2 = '标的资产债权本息余额'\n        }else if (this.form.secondary.indexOf('RedemptionDebtCount')>-1){\n          n2 = '赎回（标的资产债权笔数）'\n        }else if (this.form.secondary.indexOf('RedemptionDebtAmount')>-1){\n          n2 = '赎回（标的资产债权金额）'\n        }else if (this.form.secondary.indexOf('DebtPercentage')>-1){\n          n2 = '赎回（标的资产债权占比）'\n        }\n\n        var ydata = []\n        ydata.push({\n          type:form.primaryLine,\n          name:n1,\n          data:yz1\n        })\n        ydata.push({\n          type:form.secondaryLine,\n          name:n2,\n          data:yz2,\n          yAxis: 1\n        })\n        this.columnCart(this.domIndex,xz,ydata)\n      },\n      onSubmit(index) {\n        if (this.cartName == 'pie') {\n          if (this.form.primary == '') {\n            this.$message('请选择数值')\n          }else{\n            var data = this.distributionData[this.domIndex].slice(0,-1)\n            for (var i = 0; i < data.length; i++) {\n              data[i].y = Number(data[i][this.form.primary])\n            }\n            var pieTitle = ''\n            if (this.form.primary.indexOf('Amount')>-1) {\n              pieTitle = (this.distributionData[this.domIndex][this.distributionData[this.domIndex].length - 1].Amount).toLocaleString() + '元'\n            }else {\n              pieTitle = this.distributionData[this.domIndex][this.distributionData[this.domIndex].length - 1].Count + '笔'\n            }\n            console.log(data)\n            this.paintCart(this.domIndex, data, pieTitle)\n            this.closeEdit(index)\n          }\n        }else{\n          if (this.form.primary == '') {\n            this.$message('请选择主坐标轴')\n          }else if(this.form.secondary == '') {\n            this.$message('请选择次坐标轴')\n          }else{\n            this.columnData(this.domIndex,this.cartName,this.form)\n            this.closeEdit(index)\n          }\n        }\n\n      },\n      onSubmitNPL() {\n        if (this.cartName == 'pie') {\n          if (this.form.primary == '') {\n            this.$message('请选择数值')\n          }else{\n            var data = this.distributionDataNPLs[this.domIndex]\n            for (var i = 0; i < data.length; i++) {\n              data[i].y = Number(data[i][this.form.primary])\n            }\n            var pieTitle = ''\n            if (this.form.primary.indexOf('LoanCount')>-1) {\n              pieTitle = '户数'\n            }else if (this.form.primary.indexOf('AssetDebtCount')>-1){\n              pieTitle = '标的资产债权笔数'\n            }else if (this.form.primary.indexOf('AssetDebtAmount')>-1){\n              pieTitle = '标的资产债权本息余额'\n            }else if (this.form.primary.indexOf('RedemptionDebtCount')>-1){\n              pieTitle = '赎回（标的资产债权笔数）'\n            }else if (this.form.primary.indexOf('RedemptionDebtAmount')>-1){\n              pieTitle = '赎回（标的资产债权金额）'\n            }else if (this.form.primary.indexOf('DebtPercentage')>-1){\n              pieTitle = '赎回（标的资产债权占比）'\n            }\n            console.log(data)\n            this.paintCart(this.domIndex, data, pieTitle)\n            this.closeEdit(this.domIndex)\n          }\n        }else{\n          if (this.form.primary == '') {\n            this.$message('请选择主坐标轴')\n          }else if(this.form.secondary == '') {\n            this.$message('请选择次坐标轴')\n          }else{\n            this.columnDataNPL(this.domIndex,this.cartName,this.form)\n            this.closeEdit(this.domIndex)\n          }\n        }\n      },\n      strutTable(index,el) {\n        var dis = document.getElementsByClassName('rightChartBox'+index)[0].style.width\n        if (dis == '250px' || dis == '') {\n          document.getElementsByClassName('rightChartBox'+index)[0].style.width = '0'\n